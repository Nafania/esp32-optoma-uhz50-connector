# Example secrets.yaml file
# Copy this file as secrets.yaml and fill with your real data
# WiFi credentials should be in your global ESPHome secrets.yaml

# API Encryption Key (generate new one for security)
# Run: esphome wizard to generate new key, or use online generator
api_encryption_key: "GENERATE_NEW_32_CHAR_BASE64_KEY_HERE"

# OTA (Over-The-Air) Update Password  
ota_password: "generate_new_password_here"

# Access Point (Fallback WiFi) Configuration
ap_ssid: "Projector-Connector-Fallback"
ap_password: "generate_ap_password" 